{
  "name": "food-transparency-portal",
  "version": "1.0.0",
  "description": "Food Transparency Portal - Social Welfare Department",
  "private": true,
  "engines": {
    "node": "18.x || 20.x",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.7",
    "axios": "^1.13.2",
    "chart.js": "^3.9.1",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.1",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "lucide-react": "^0.263.1",
    "mongoose": "^8.18.1",
    "multer": "^2.0.2",
    "postcss": "^8.4.14",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "serve": "^14.2.5",
    "tailwindcss": "^3.1.8",
    "uuid": "^13.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "postinstall": "node scripts/fix-dependencies.js",
    "install:clean": "rm -rf node_modules package-lock.json && npm cache clean --force && npm install --legacy-peer-deps --no-audit",
    "build:render": "npm run install:clean && npm run postinstall && npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6"
  },
  "overrides": {
    "schema-utils": {
      "ajv": "^6.12.6",
      "ajv-keywords": "^3.5.2"
    },
    "ajv-formats": {
      "ajv": "^8.12.0"
    },
    "terser-webpack-plugin": {
      "ajv-formats": "^2.1.1"
    }
  }
}
